{
	"productName": "BlueBuzzah Updater",
	"version": "1.0.0",
	"identifier": "com.bluebuzzah.updater",
	"build": {
		"beforeDevCommand": "npm run dev",
		"beforeBuildCommand": "npm run build",
		"devUrl": "http://localhost:5173",
		"frontendDist": "../dist"
	},
	"app": {
		"withGlobalTauri": true,
		"windows": [
			{
				"title": "BlueBuzzah Updater",
				"width": 900,
				"height": 750,
				"minWidth": 800,
				"minHeight": 700,
				"resizable": true,
				"fullscreen": false,
				"decorations": true,
				"center": true
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		],
		"macOS": {
			"minimumSystemVersion": "10.13"
		}
	},
	"plugins": {}
}